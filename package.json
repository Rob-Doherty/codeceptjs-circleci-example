{
  "name": "codeceptjs-example",
  "version": "1.2.0",
  "description": "Example usage of CodeceptJS test framework",
  "main": "index.js",
  "repository": "git@github.com:Rob-Doherty/codeceptjs-example.git",
  "author": "Rob Doherty-Kelly",
  "license": "MIT",
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
